Función ingresos <- tomar_ingresos
	// Definir tipo de varible
	Definir ingresos Como Real
	// 2 tomar datos o asignar varibles
	Escribir 'ingreso bruto de $2.000.000'
	ingresos <- 2000000
FinFunción

Función ganancia <- tomar_gastos(ingresos)
	// Definir tipo de varibles
	Definir gastos, ganancia Como Real
	// tomar datos o asignar varibles
	Escribir 'Ingrese cuanto gasto $: '
	Leer gastos
	// 3 procesar datos
	ganancia <- ingresos-gastos
FinFunción

Función imprimir_ganancia(ganancia)
	// 4  mostrar resultados
	Escribir ' la ganancia neta es de: $', ganancia
FinFunción

Algoritmo calcular_ganancias
	Definir ingresos, ganancia Como Real
	ingresos <- tomar_ingresos()
	ganancia <- tomar_gastos(ingresos)
	
	imprimir_ganancia(ganancia)
FinAlgoritmo
